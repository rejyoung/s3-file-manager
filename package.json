{
    "name": "s3-file-manager",
    "version": "1.0.0",
    "description": "A streamlined, high-level S3 client for Node.js with built-in retries and support for uploads, downloads, and file operations â€” works with any S3-compatible storage.",
    "engines": {
        "node": ">=18.0.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/rejyoung/s3-file-manager.git"
    },
    "homepage": "https://github.com/rejyoung/s3-file-manager#readme",
    "bugs": {
        "url": "https://github.com/rejyoung/s3-file-manager/issues"
    },
    "sideEffects": false,
    "main": "dist/esm/index.js",
    "types": "dist/types/index.d.ts",
    "files": [
        "dist",
        "README.md",
        "CONTRIBUTING.md",
        "LICENSE"
    ],
    "exports": {
        ".": {
            "types": "./dist/types/index.d.ts",
            "import": "./dist/esm/index.js",
            "require": "./dist/cjs/index.js"
        },
        "./file-type-wrapper": {
            "import": "./dist/esm/file-type-wrapper/file-type-wrapper.mjs",
            "require": "./dist/cjs/file-type-wrapper/file-type-wrapper.cjs"
        }
    },
    "scripts": {
        "build:esm": "tsc --outDir dist/esm --module ESNext && cp -R src/file-type-wrapper dist/esm/file-type-wrapper",
        "build:cjs": "tsc --outDir dist/cjs --module CommonJS && cp -R src/file-type-wrapper dist/cjs/file-type-wrapper",
        "build": "npm run build:esm && npm run build:cjs",
        "start": "npm run build && node dist/esm/index.js",
        "prepublishOnly": "npm run build",
        "test": "vitest"
    },
    "keywords": [
        "aws",
        "s3",
        "s3-client",
        "file-manager",
        "object-storage",
        "file-upload",
        "file-download",
        "s3-wrapper",
        "node-s3",
        "s3-utility",
        "upload-manager",
        "download-manager",
        "buffer",
        "streaming",
        "multipart-upload",
        "retry-logic",
        "aws-sdk"
    ],
    "author": "Ryan Young",
    "license": "MIT",
    "type": "module",
    "devDependencies": {
        "@types/is-utf8": "^0.2.3",
        "@types/mime-types": "^3.0.1",
        "@types/node": "^24.0.10",
        "aws-sdk-client-mock": "^4.1.0",
        "ts-node": "^10.9.2",
        "typescript": "^5.8.3",
        "vitest": "^3.2.4"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.842.0",
        "@aws-sdk/s3-request-presigner": "^3.842.0",
        "bottleneck": "^2.19.5",
        "file-type": "^21.0.0",
        "is-utf8": "^0.2.1",
        "mime-types": "^3.0.1"
    }
}
